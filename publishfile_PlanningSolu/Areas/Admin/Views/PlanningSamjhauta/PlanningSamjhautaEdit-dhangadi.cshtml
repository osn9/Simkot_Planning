<script>
    function setsadashya() {
        var Sadashya = $('#Sadashya');
        $(Sadashya).val('');
        var SadashyaVal = '';
        $('.Sadashya').each(function (index, item) {
            SadashyaVal += $(item).val() + ','
        });
        SadashyaVal = SadashyaVal.substring(0, SadashyaVal.length - 1);
        $(Sadashya).val(SadashyaVal);
    }
</script>






