<script>
    $(document).ready(function () {
        $('.shrot').keyup(function () {
            debugger;
            var calculated_total_sum = 0;
            $('.shrot').each(function () {
                var get_textbox_value = $(this).val();
                if ($.isNumeric(get_textbox_value)) {
                    calculated_total_sum += parseFloat(get_textbox_value);
                }
            });
            $("#TotalSum").html(calculated_total_sum);
            $("#subTotalSourceAmount").val(calculated_total_sum);
        })
    });
</script>


<script>
    $(document).ready(function () {
        $('.kistarakam').keyup(function () {
            debugger;
            var calculated_total_sum = 0;
            var katta_hune_rakam = 0;
            $('.kistarakam').each(function () {
                var get_textbox_value = $(this).val();
                if ($.isNumeric(get_textbox_value)) {
                    calculated_total_sum += parseFloat(get_textbox_value);

                    katta_hune_rakam = parseFloat(calculated_total_sum * 0.03)
                }
            });

            $("#kattaHuniRakam").html(katta_hune_rakam);

            $("#TotalSumForKista").html(calculated_total_sum - katta_hune_rakam);

            var totamt = calculated_total_sum - katta_hune_rakam;

            //document.getElementById('TotalSumInWord').value = totamt;

            $('#subTotalKattaHuni').val(katta_hune_rakam);
            $('#subTotalKista').val(totamt);

        })
    });
</script>




