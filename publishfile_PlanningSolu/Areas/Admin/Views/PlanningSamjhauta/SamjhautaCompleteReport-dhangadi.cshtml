
@model Planning.Planning.Entities.Planning_Samjhauta
@{
    ViewBag.Title = "SamjhautaCompleteReport";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";

    
        List<Planning.Planning.Entities.Organization_Setup> OrganiationName = new List<Planning.Planning.Entities.Organization_Setup>();
        using (Planning.Planning.Entities.PLANNING_OCT_03Entities db = new Planning.Planning.Entities.PLANNING_OCT_03Entities())
        {
            OrganiationName = (from i in db.Organization_Setup select i).ToList();
        }
   

}

@using Planning.Helpers



<script src="~/Scripts/jquery.maskedinput.js"></script>

    <div class="padding-md">
        <section style="background:#efefe9;">
            <div class="container">
                <div class="row">
                    <div class="board">
                        <div class="tab-content">
                            <div class="tab-pane fade in active" id="fiscalYearRecord">

                                <input type="button" class="btn btn-primary pull-right" value="सम्झौता प्रिन्ट गर्नुहोस" formtarget="_blank" onclick="javascript: printDiv('printdiv')" />

                                <div class="main-content" id="printdiv">


                                    <div class="row header-section">
                                        <div class="col-md-2" style="display:inline-block; position:relative;">
                                            <img src="~/img/gov-logo.png" style="max-height:160px; max-width:100%; display:inline-block; position:relative; vertical-align:baseline;" />
                                        </div>
                                        <div class="col-md-10 text-center" style="display:inline-block; position:relative;">
                                            @if (OrganiationName.Count > 0)
                                            {
                                                using (Planning.Planning.Entities.PLANNING_OCT_03Entities db = new Planning.Planning.Entities.PLANNING_OCT_03Entities())
                                                {
                                                    foreach (Planning.Planning.Entities.Organization_Setup i in OrganiationName)
                                                    {
                                                        <h1>@i.Organization_Name</h1>
                                                        <h2>@i.Organization_Type</h2>
                                                        <h3>@i.District_Name</h3>
                                                        <h4>@i.State_Country</h4>
                                                    }
                                                }
                                            }
                                        </div>
                                    </div>
                                    <hr />


                                    <div class="col-md-12">
                                        <div style="text-align:center; font-weight:bold;">
                                            <h3 style="text-align:center; font-weight:bold;">योजना सम्झौता फारम</h3>
                                        </div>
                                    </div>


                                    @foreach (var item in Model.Planning_Entry)
                                    {
                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">आर्थिक वर्षः<span> @item.FiscalYear </span></div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">
                                                    वजेट श्रोत: @if (item.Budget_Source == 1)
                                                    {<span>वितिय समानिकरण </span>}
                                                    else if (item.Budget_Source == 2)
                                                    { <span>प्रदेश सरकार </span>}
                                                    else if (item.Budget_Source == 3)
                                                    {<span> आन्तरिक श्रोत</span> }
                                                </div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">
                                                    वडा स्तरिय/नगर स्तरिय:  @if (item.Planning_Type == "O")
                                                    {<span> वडा स्तरिय</span> }
                                                    else if (item.Planning_Type == "N")
                                                    { <span> नगर स्तरिय</span>}
                                                </div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">वडा नं.:<span> @Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Ward_No.ToString()) </span></div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">
                                                    कार्यक्षेत्र: @if (item.Work_Area == 1)
                                                    {<span>पूर्वाधार</span> }
                                                    else if (item.Work_Area == 2)
                                                    {<span>सामाजिक</span> }
                                                    else if (item.Work_Area == 3)
                                                    {<span>आर्थिक</span>  }
                                                    else if (item.Work_Area == 4)
                                                    { <span>वातावरण</span> }
                                                </div>
                                            </div>
                                            <div class="clearfix"></div>
                                        </div>

                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>आयोजनाः सम्बनधी विवरण :</b> </div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">योजनाको नाम र स्थान : <span>@Html.DisplayFor(model => item.Planning_Name, new { @class = "form-control" })</span></div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">योजना शुरु हुने मिति : @if (item.Planning_Start_Date != null)
                                                {<span> @Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Planning_Start_Date.Value.ToShortDateString()) </span>}
                                                else {<span>&nbsp;</span> }</div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">योजना सम्पन्न हुने मिति : @if (item.Planning_End_Date != null)
                                                {<span>@Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Planning_End_Date.Value.ToShortDateString())</span>}
                                                else {<span>&nbsp;</span> }</div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">कामको विवरण : <span> @Html.DisplayFor(model => item.Work_Details, new { @class = "form-control" })</span></div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">लागत इष्टिमेट अङ्क रु : <span> @Html.DisplayFor(model => item.Amount_Estimate, new { @class = "form-control" })</span></div>
                                                <div class="col-lg-12 col-md-12" style="font-size:16px;">कामको किसिम : @if (item.Work_Type == 1) {<span>उपभोक्ता समिति</span> } else if (item.Work_Type == 2) {<span>ठेक्का पट्टा</span> } else if (item.Work_Type == 3) {<span>अमानत</span> }</div>
                                            </div>
                                            <div class="clearfix"></div>
                                        </div>
                                        
                                    }


                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                        <div class="row">
                                            @foreach (var anudan in Model.Planning_Entry_Anudan)
                                            {
                                            <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;">अनुदान : </div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px;">क. धनगढीमाई नगरपालिका : <span>@anudan.Nagarpalika_Amt</span></div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px;">ख. उ.स. : <span> @anudan.UpaBhoktaSamiti_Amt </span></div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px;">ग. अन्य : <span>@anudan.Other_Amt</span></div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px;">घ. जनसहभागीता : <span>@anudan.JanaSahaBhagita_Amt </span></div>
                                            }
                                            @foreach (var ite in Model.Planning_Entry)
                                            {
                                            <div class="col-lg-12 col-md-12" style="font-size:16px;">ङ. गेेर सरकारी संघ/संस्था : <span></span></div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px; margin-left:50px;">उपभोक्ता समितिको अध्यक्षको नाम र ठेगाना : <span>@ite.UpaBhoktaSamiti_HeadName</span></div>
                                            <div class="col-lg-12 col-md-12" style="font-size:16px; margin-left:50px;">ठेकेदारको नाम र ठगाना : <span>@ite.Contractor_Name</span></div>
                                            }
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>

                                    <p>&nbsp;</p>

                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>ठेकेदारले न.पा. बाट प्रप्त गर्ने किस्ता विवरण :</b> </div>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                        <div class="row">
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>किस्ता क्रम</b> </div>
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>किस्ता रकम</b> </div>
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>मिति</b> </div>
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>पेश्की / भुक्तानी</b> </div>
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>शर्त</b> </div>
                                            <div class="col-lg-2 col-md-2" style="font-size:16px; font-weight:600;"><b>केेफियत</b> </div>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>

                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                        @foreach (var item in Model.Planning_Entry_Kista_Detail)
                                        {
                                        <div class="row">
                                                <div class="col-lg-2 col-md-2" style="font-size:16px;"><span>@item.Kista_Kram</span></div>
                                                <div class="col-lg-2 col-md-2" style="font-size:16px;"><span>@item.Kista_Rakam</span></div>
                                                <div class="col-lg-2 col-md-2" style="font-size:16px;">@if (item.Payment_Date != null)
                                                {<span>@Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Payment_Date.Value.ToShortDateString())</span>}</div>
                                                <div class="col-lg-2 col-md-2" style="font-size:16px;"><span>@item.Peski_Bhuktani</span></div>
                                                <div class="col-lg-3 col-md-3" style="font-size:16px;"><span>@item.Sarta</span></div>
                                                <div class="col-lg-3 col-md-3" style="font-size:16px;"><span>@item.Remarks</span></div>
                                            <div class="clearfix"></div>
                                        </div>
                                        }
                                    </div>


                                        <p>&nbsp;</p><p>&nbsp;</p>

                                        <div class="col-md-12">
                                            <div style="text-align:center; font-weight:bold;">
                                                <h3 style="text-align:center; font-weight:bold;">धनगढीमाई नगरपालिका कार्यालय र उपभोक्ता समिति बीचको सम्झौता</h3>
                                                <h4 style="text-align:center; font-weight:bold;">अनुसूची ..... सँग सम्बन्धित</h4>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>

                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;">
                                            सम्झौता गर्ने संस्थाको नामः @Html.DisplayFor(model => model.Samjhauta_Org_Name, new { @class = "form-control" })
                                        </div>

                                        <p>&nbsp;</p>



                                        <div class="col-lg-12 col-md-12" style="font-size:18px; font-weight:bold;"><h4><b>१. सम्झौत गर्ने पक्ष र आयोजनाः</b></h4></div>

                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                @foreach (var item in Model.Organization_Representative)
                                                {
                                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>(क) उपभोक्त समिति /समुदायमा आधारीत संस्था /गेेर सरकारी संस्थाको विवरण : सम्झौता गर्ने संस्थाको प्रतिनिधीको,</b> </div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">१. नामः <span> @Html.DisplayFor(model => item.Representative_Name, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">२. पदः <span> @Html.DisplayFor(model => item.Representative_Designition, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">३. ठेगानाः <span> @Html.DisplayFor(model => item.Representative_Address, new { @class = "form-control" })</span></div>
                                                }
                                                <div class="clearfix"></div>
                                            </div>
                                        </div>



                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>(ख) आयोजनाको विवरणः</b> </div>
                                                @foreach (var item in Model.Project_Entry_Detail)
                                                {

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">१. नामः <span> @Html.DisplayFor(model => item.Project_Name, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">२. स्थानः <span> @Html.DisplayFor(model => item.Project_Place, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">३. उदेश्य: <span> @Html.DisplayFor(model => item.Project_Objective, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">४. आयोजना स्वीकृत गर्ने निकायः <span> @Html.DisplayFor(model => item.Project_Acceptance_By, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">५.आयोजना शुरु हुने मितिः @if (item.Project_Start_Date!=null)
                                                    {<span> @Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Project_Start_Date.Value.ToShortDateString()) </span>}
                                                    else {<span>&nbsp;</span> }</div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">६.आयोजना सम्पन्न हुने मितिः @if (item.Project_End_Date!=null)
                                                    {<span> @Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Project_End_Date.Value.ToShortDateString()) </span>}
                                                    else
                                                    {<span>&nbsp;</span> }</div>


                                                }
                                                <div class="clearfix"></div>
                                            </div>
                                        </div>


                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                @*<div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>(ख) आयोजनाको विवरणः</b> </div>*@
                                                @foreach (var item in Model.Project_Source_Detail)
                                                {

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">(क) लागत अनुमान रुः <span> @Html.DisplayFor(model => item.Project_estimated_Amount, new { @class = "form-control" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">(ख) लागत व्यहोर्ने श्रोतहरुः <span></span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">१. नेपाल सरकार <span> रु.@Html.DisplayFor(model => item.Nepal_Government, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">२. नगरपालिकावाट <span> रु.@Html.DisplayFor(model => item.Municipality, new { @class = "form-control shrot" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">३. प्रदेश <span> रु.@Html.DisplayFor(model => item.State, new { @class = "form-control shrot" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">४. गेेर सरकारी संघ संस्थाबाट <span> रु.@Html.DisplayFor(model => item.NGO_INGO, new { @class = "form-control shrot" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">५. समुदायमा आधारित संस्थाबाट <span> रु.@Html.DisplayFor(model => item.Community_Org, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">६. विदेशी दातृ संघ संस्थाबाट <span> रु.@Html.DisplayFor(model => item.Foreign_Org, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">७. उपभोक्ता समितिवाट नगद <span> रु.@Html.DisplayFor(model => item.Public_Community, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">श्रमदान <span> रु.@Html.DisplayFor(model => item.Loan_Grant, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">८. अन्यवाट <span> रु.@Html.DisplayFor(model => item.Other_Source, new { @class = "form-control shrot" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>जम्मा रु. <span id="TotalSum"></span></b></div>

                                                }
                                                <div class="clearfix"></div>
                                            </div>
                                        </div>

                                        <div class="clearfix"></div>

                                        <div class="col-lg-12 col-md-12" style="font-size:18px; font-weight:bold;"><h4><b>२. आयोजनावाट लाभान्वित हुने (प्रतिशतमा):</b></h4></div>

                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">

                                                @foreach (var item in Model.Beneficiaries_Group)
                                                {

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">१. घर संख्याः <span> @Html.DisplayFor(model => item.Total_House, new { @class = "form-control" }) </span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">२. जनसंख्याः <span> महिला:- @Html.DisplayFor(model => item.Total_Male, new { @class = "form-control" }) जना</span>&nbsp;&nbsp;&nbsp;<span>पुरुष:- @Html.DisplayFor(model => item.Total_Female, new { @class = "form-control" }) जना</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">३. समुदायः <span> @Html.DisplayFor(model => item.Community, new { @class = "form-control" })</span></div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">४. अन्यः- समुदायमा आधारीत / गेेर सरकारी संस्थाः <span> @Html.DisplayFor(model => item.Other, new { @class = "form-control" })</span></div>

                                                }

                                                <div class="clearfix"></div>
                                            </div>
                                        </div>

                                        <div class="clearfix"></div>


                                        <div class="col-lg-12 col-md-12" style="font-size:18px; font-weight:bold;"><h4><b>३. उपभोक्त समिति /गेेर सरकारी संस्था /समुदायमा आधारीत संस्था सम्बन्धि विवरण :</b></h4></div>

                                        <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:500; padding-left:60px; line-height:30px;">
                                            <div class="row">
                                                @foreach (var item in Model.Samiti_Detail)
                                                {
                                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>(क) समिति गठन भएको मितिः</b> @if (item.Samiti_Estd_Date != null)
                                                    {<span>@Planning.Utility.NepaliEnglishNumber.English_Nepali(item.Samiti_Estd_Date.Value.ToShortDateString())</span>}
                                                    else {<span>&nbsp;</span> } </div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px; font-weight:600;"><b>(ख) पदाधिकारीको नाम र ठेगानाः</b> </div>

                                                    <div class="col-lg-12 col-md-12" style="font-size:16px;">१. अध्यक्षः <span> @Html.DisplayFor(model => item.Adakshya, new { @class = "form-control" }) </span></div>

