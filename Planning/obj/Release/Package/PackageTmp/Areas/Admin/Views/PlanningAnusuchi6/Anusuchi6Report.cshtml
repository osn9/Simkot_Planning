
@model Planning.Areas.Admin.Models.PlanningAnusuchiViewModel.Anusuchi6ViewModel

@{
    ViewBag.Title = "Anusuchi6Report";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<style>
    .cotract_letter {
        border: 1px solid #ccc;
        margin: 30px 0px;
        padding: 30px;
    }

        .cotract_letter header-sec img {
            max-width: 100%;
            max-height: 100px;
        }

    .header-section h1 {
        font-size: 65px;
        letter-spacing: 5px;
    }

    .header-section h2 {
        font-size: 40px;
    }

    .header-section h3, .subject h3 {
        font-size: 30px;
    }

    .header-section h4 {
        font-size: 25px;
        margin-top: 15px;
    }

    .header-section h1, .header-section h2, .header-section h3,
    .header-section h4, .subject h3, .letter_footer h3, .letter_content h4, .aanya-raaya h3 {
        font-weight: 600;
    }

    .subject h3 {
        margin-top: 60px;
    }

    .letter_footer {
        margin-top: 60px;
    }

    .approved-by {
        margin-top: 60px;
    }

    .letter_footer, .letter_footer strong, .letter_content, .titleNumber {
        font-size: 18px;
        font-weight: 500;
    }

        .letter_footer ul {
            margin-top: 15px;
        }

            .letter_footer ul li {
                list-style-type: none;
            }

                .letter_footer ul li span {
                    padding-left: 20px;
                    vertical-align: middle;
                    display: inline-block;
                    position: relative;
                    margin-top: -14px;
                }

        .titleNumber ul li {
            list-style-type: none;
        }

    .personName {
        border-style: none;
    }

        .personName:hover {
            border-style: none;
        }

        .personName:focus {
            border-style: none;
        }

    .btn.btn-primary.pull-right {
        margin-top: 15px;
        margin-right: 15px;
    }
</style>

@foreach (var item in Model.Anusuchi6ViewModelList)
{
    <div class="container">
        <input type="button" class="btn btn-primary pull-right" value="खर्च फारम प्रिन्ट गर्नुहोस" formtarget="_blank" onclick="javascript: printDiv('printdiv')" />
        <div class="cotract_letter" id="printdiv">

            <style>
                .header-section .table tbody > tr, .content-section .table tbody > tr, .detail .table tbody > tr, .sign.table tbody > tr {
                    border: none !important;
                }

                header-section .table tr td, .content-section .table tbody > tr, .detail .table tbody > tr, .sign .table tbody > tr {
                    border: none !important;
                    border-color: #f9f9f9 !important;
                    padding: 0px !important;
                }

                @@media print {
                    .header-section .table tr td, .content-section .table tr td {
                        border: none !important;
                        border: !important;
                        border-color: #fff !important;
                        padding: 0px !important;
                    }

                    .header-section .table tbody > tr, .content-section .table tbody > tr {
                        border: none !important;
                        border: 0 !important;
                    }

                    .header-section .table, .table tbody > tr, td, .content-section .table, .table tbody > tr {
                        border: none !important;
                        border: 0 !important;
                    }
                }
            </style>

            <div class="header-section">
                <table class="table" border="0" style="border:0px !important;" cellpadding="0" cellspacing="0">
                    <tbody>
                        <tr>
                            <td width="25%" style="border:none !important; border:0px !important;">
                                <img src="~/img/gov-logo.png" style="max-height:120px; max-width:100%; display:inline-block; position:relative; vertical-align:baseline;" />
                            </td>
                            <td width="45%" style="text-align:center; border:none !important; border:0px !important;">
                                <h2 style="font-weight:bold; font-size:14px;">अनुसूची ६</h2>
                                <h2 style="font-weight:bold; font-size:14px; margin-top:0px;">(कार्यविधिको दफा १६ (ङ) सँग सम्बन्धित)</h2>
                                <h2 style="font-weight:bold; font-size:14px; margin-top:0px;">उपभोक्ता समितिको भोेतिक तथा वित्तिय प्रगति प्रतिवेदन</h2>
                                <h2 style="font-weight:bold; font-size:14px; margin-top:0px;">विवरण पेश गरेको कार्यालय...................</h2>
                            </td>
                            <td width="30%" style="border:none !important; border:0px !important;">
                                <img src="~/img/nagarpalika_logo.jpg" style="max-height:120px; max-width:100%; display:inline-block; position:relative; vertical-align:baseline; float:right;" />
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>

           
            <div class="col-lg-12 col-md-12" style="font-size:14px;">१. आयोजनाको विवरण</div>
            <div class="col-lg-12 col-md-12" style="font-size:14px; font-weight:500; padding-left:60px; line-height:30px;">
                <div class="row">
                    <div class="col-md-4" style="display:inline-block; position:relative; vertical-align:top;">
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">आयोजनाको नामः <span>@item.Project_Name </span></div>
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">उपभोक्ता समिति: <span>@item.Upabhokta_Samiti_Name </span></div>
                    </div>
                    <div class="col-md-4" style="display:inline-block;position:relative;">
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">वडा नं.: <span> @item.Ward_No </span></div>
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">अध्यक्षः <span> @item.Adakshya </span></div>
                    </div>
                    <div class="col-md-4" style="display:inline-block;position:relative;">
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">टोल-बस्तीः <span>@item.Street_Name </span></div>
                        <div class="col-lg-12 col-md-12" style="font-size:14px;">सचिवः <span> @item.Sachib </span></div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">२. आयोजनाको लागतः प्राप्त अनुदान रकम रु.&nbsp;&nbsp;<strong>@item.Anudan_Rakam</strong>&nbsp;&nbsp; चन्दा रकम रु.&nbsp;&nbsp;<strong>@item.Chanda_Rakam</strong>&nbsp;&nbsp; जनसहभागिता रकम रु. &nbsp;&nbsp;<strong>@item.Jana_Sahabhagita_Rakam</strong>&nbsp;&nbsp;  जम्मा रकम रु.&nbsp;&nbsp;<strong>@item.Total_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">३. हालसम्मको खर्च रकम रु.&nbsp;&nbsp;<strong>@item.Total_Expenses_TillNow</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">क. कार्यालयबाट प्राप्त रकम रु.&nbsp;&nbsp;<strong>@item.Karyalaya_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">१. निर्माण सामग्रीमा(सिमेन्ट,छड,काठ,ढुंगा वा फुवा,गिट्टी,उपकरण आदि) रु.&nbsp;&nbsp;<strong>@item.Nirman_Samagri_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">२. ज्यालाः- दक्षः रु.&nbsp;&nbsp;<strong>@item.Dashya_Amount</strong>&nbsp;&nbsp;&nbsp;&nbsp;अदक्षः रु.&nbsp;&nbsp;<strong>@item.Adashya_Amount</strong>&nbsp;&nbsp;&nbsp;&nbsp;जम्मा रु.&nbsp;&nbsp;<strong>@item.Dashya_Amount +  @item.Adashya_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">३. मसलन्द सामान(कपि,कलम,मसी,कागज आदि) रु.&nbsp;&nbsp;<strong>@item.Masalanda_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">४. दैनिक भ्रमण भत्ता(सम्झोेतामा स्वीकृत भए) रु.&nbsp;&nbsp;<strong>@item.Travel_Expenses_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">५. प्राविधिक निरीक्षण बापत खर्च(सम्झोेतामा स्वीकृत भए) रु.&nbsp;&nbsp;<strong>@item.Technical_Supervise_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">६. अन्य &nbsp;&nbsp;<strong>@item.Others_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">ख. जनसहभागिताबाट व्यहोरिएको खर्च रु. &nbsp;&nbsp;<strong>@item.Janasahabhagita_Amount</strong>&nbsp;&nbsp; श्रमको मूल्य बराबर रकम रु. &nbsp;&nbsp;<strong>@item.Sharam_Amount</strong>&nbsp;&nbsp; जिन्सी सामान मूल्य बराबर रकम रु. &nbsp;&nbsp;<strong>@item.Jinsi_Amount</strong>&nbsp;&nbsp; कुल जम्मा रु.&nbsp;&nbsp;<strong></strong>&nbsp;&nbsp;</div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">७. प्राविधिक प्रतिवेदन बमोजिम मुल्यांकन रकम रु. &nbsp;&nbsp;<strong>@item.Technical_Review_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">८. उपभोक्ता समितिको निर्णय बमोजिम-समीक्षाबाट खर्च देखिएको रु. &nbsp;&nbsp;<strong>@item.Upabhikta_Decision_Amount</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">९.सामाधानका उपायहरु: </div>

            @{ int s = 1;}
            @foreach (var sol in Model.Anusuchi6_SolutionsViewModelList)
            {
                <div class="col-lg-12 col-md-12" style="font-size:14px; margin-left:30px;">@Planning.Utility.NepaliEnglishNumber.English_Nepali(s.ToString()).<span>@sol.Solutions </span></div>
                s++;
            }

            <div class="col-lg-12 col-md-12" style="font-size:14px;">१०. कार्यालयबाट र अन्य निकायबाट अनुगमन भए अनुगमनको सुझाव &nbsp;&nbsp;<strong>@item.Field_Supervise_Decision</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">११. हाल माग गरेको किस्ता रकम रु. &nbsp;&nbsp;<strong>@item.Kista_Rakam_Demand</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">१२. मुख्य खर्च प्रयोजन &nbsp;&nbsp;<strong>@item.Main_Expenses</strong>&nbsp;&nbsp; </div>

            <div class="col-lg-12 col-md-12" style="font-size:14px;">१३. प्राप्त रकम आयोजना बाहेक अन्य कार्यमा खर्च गर्ने गराउने छैनोे ।</div>


            <br /><br /><br /><br /><br /><br /><br /><br />

            <div class="sign">
                <table class="table" border="0" style="border:0px !important;" cellpadding="0" cellspacing="0">
                    <tbody>
                        <tr>
                            <td>...................................</td>
                            <td>...................................</td>
                            <td>...................................</td>
                        </tr>
                        <tr>
                            <td style="font-size:14px; border:none !important;"><span style="margin-top:0px; display:block;">कोषाध्यक्ष</span></td>
                            <td style="font-size:14px; border:none !important;"><span style="margin-top:0px; display:block;">सचिव</span></td>
                            <td style="font-size:14px; border:none !important;"><span style="margin-top:0px; display:block;">अध्यक्ष</span></td>
                        </tr>
                    </tbody>
                </table>
            </div>



            <div class="clearfix"></div>
        </div>
    </div>
}


<script language="javascript" type="text/javascript">
    function printDiv(divID) {
        var divElements = document.getElementById(divID).innerHTML;
        var oldPage = document.body.innerHTML;
        document.body.innerHTML =
          "<html><head><title></title></head><body>" +
          divElements + "</body>";
        window.print();
        document.body.innerHTML = oldPage;
    }
</script>