
@model Planning.Areas.Admin.Models.PlanningAnusuchiViewModel.Anusuchi3ViewModel

@using Planning.Helpers




@{ int i = 1;}

@using (Html.BeginCollectionItem("Anusuchi3WorkDivisionViewModelList"))
{
    <div id="Anusuchi3MembersFormgroup" class="form-group">
        <div class="col-md-5"><label>सदस्य र जिम्मेवारी</label>@Html.TextBoxFor(model => model.Members, new { @class = "form-control" }) </div>
        <div class="btn-delete col-md-1">
            <div><a href='javascript:;' class="btn btn-danger btn-sm removemembers" style="margin-top:35px;"><span class='fa fa-minus fa-1x'></span></a></div>
        </div>
        <div class="clearfix"></div>
    </div>
}


<script>
    $(".removemembers").click(function () {
        $(this).parents("#Anusuchi3MembersFormgroup").remove();
    });
</script>


