                        @Html.EditorFor(model => model.Anudan_Rakam, new { htmlAttributes = new { @class = "form-control nepali-calendar" } })
                        @Html.ValidationMessageFor(model => model.Anudan_Rakam, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="form-group">
                        <label>चन्दा रकमः</label>
                        @Html.EditorFor(model => model.Chanda_Rakam, new { htmlAttributes = new { @class = "form-control nepali-calendar" } })
