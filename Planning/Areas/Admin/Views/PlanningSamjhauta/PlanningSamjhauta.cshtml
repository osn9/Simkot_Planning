<script>    
    function getTotalContigency() {
        debugger;
        var municipalityAmt= $('#Municipality1').val();
        var nepAmt = $('#Nepal_Government1').val();
        var pradeshAmt = $('#State1').val();
        if (isNaN(municipalityAmt)) {
            municipalityAmt = 0;
        }
        if (isNaN(nepAmt)) {
            nepAmt = 0;
        }
        if (isNaN(pradeshAmt)) {
            pradeshAmt = 0;
