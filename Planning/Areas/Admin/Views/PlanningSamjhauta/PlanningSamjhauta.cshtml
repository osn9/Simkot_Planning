        if (nepAmt === '') {
            nepAmt = 0;
        }
        if (pradeshAmt === '') {
            pradeshAmt = 0;
        }
        
        var Amount = parseFloat(municipalityAmt) + parseFloat(nepAmt) + parseFloat(pradeshAmt);
        //alert(Amount);
        var contigency = $('#conti').val();
        //var contiValue = parseFloat((contigency / 100) * municipalityAmt);
        var contiValue = parseFloat((contigency / 100) * Amount);
        var fixcontiValue = contiValue.toFixed(2);
        $.ajax({
            url: "/PlanningSamjhauta/Get_English_Nepali",
            type: 'GET',
            data: {
                EnglishNumericValue: fixcontiValue
            },
            success: function (result) {
                $('input[id=Contegency_Amount]').val(result);
                var totalAmount = parseFloat(Amount) - parseFloat(contiValue);
                $('input[id=Total_Amount]').val(totalAmount); 
                $('#Total_Amount_span').html(totalAmount); 


                getTotalMarmat();
            },
            error: function () {
            }
        });
    }
