            nepAmt = 0;
        }
        if (isNaN(pradeshAmt)) {
            pradeshAmt = 0;
        }
        if (municipalityAmt === '') {
            municipalityAmt = 0;
        }
        if (nepAmt === '') {
            nepAmt = 0;
        }
        if (pradeshAmt === '') {
            pradeshAmt = 0;
        }
        var Amount = parseFloat(municipalityAmt) + parseFloat(nepAmt) + parseFloat(pradeshAmt);
