<script>
	$(document).ready(function(){
		$('#nepaliDate').nepaliDatePicker({
			ndpEnglishInput: 'englishDate'
		});
		$('#englishDate').change(function(){
			$('#nepaliDate').val(AD2BS($('#englishDate').val()));
		});

		CheakWorkKism();
		var a = $("#Work_Type").change(function ()
		{
		    CheakWorkKism();
		});
		function CheakWorkKism()
		{
		    var workKism = $("#Work_Type").val();
		    if (workKism == '')
		    {
		        $("#UpbhokataKism").hide();
		        $("#ThikaPattaKism").hide();
		        $("#AmanatKism").hide();
		    }
		    if (workKism == 1)
		    {
		        $("#AmanatKism").hide();
		        $("#ThikaPattaKism").hide();
		        $("#UpbhokataKism").show();
		    } else if (workKism == 2)
		    {
		        $("#AmanatKism").hide();
		        $("#UpbhokataKism").hide();
		        $("#ThikaPattaKism").show();
		    } else if (workKism == 3)
		    {
		        $("#UpbhokataKism").hide();
		        $("#ThikaPattaKism").hide();
		        $("#AmanatKism").show();
		    }
		}
	});
</script>
